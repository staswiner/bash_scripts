highlight Normal ctermfg=white ctermbg=black
set tabstop=2
syntax on
set number
set visualbell
vmap <down> <down>$
vmap <up> <up>0
map  0i#<esc>
vmap  :norm i#<esc>
" set list
highlight Pmenu       ctermbg=LightGray
highlight PmenuSel    ctermbg=Gray
highlight PmenuSbar   ctermbg=Gray
highlight PmenuThumb  ctermbg=Black
